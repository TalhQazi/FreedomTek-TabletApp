{
  "expo": {
    "name": "FreedomTekTablet",
    "slug": "FreedomTekTablet",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "freedomtektablet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.malikfawad.FreedomTekTablet",
      "permissions": [
        "INTERNET",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
